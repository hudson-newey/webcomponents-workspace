{
  "name": "web-components-workspace",
  "private": true,
  "version": "1.3.3",
  "scripts": {
    "start": "vite --config vite.config.ts ./dist/docs/",
    "build": "rimraf build/ && pnpm build:components && pnpm build:docs",
    "build:components": "cem analyze --litelement --globs \"src/**/*.ts\" && vite build",
    "build:docs": "eleventy --config=.eleventy.cjs",
    "publish:components": "pnpm -C src/components run publish:components",
    "test:unit": "playwright test components/",
    "lint": "eslint ."
  },
  "keywords": [
    "web components",
    "components",
    "workspace"
  ],
  "customElements": "custom-elements.json",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@custom-elements-manifest/analyzer": "^0.9.2",
    "@jest/types": "^29.6.3",
    "@playwright/test": "1.41.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@sand4rt/experimental-ct-web": "^1.41.0",
    "@types/node": "^20.11.17",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^2.0.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.0.1",
    "vite": "^5.0.8"
  }
}
