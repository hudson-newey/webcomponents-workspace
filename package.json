{
  "name": "web-components-workspace",
  "private": true,
  "version": "1.7.8",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:docs": "vite ./dist/docs/",
    "build:docs": "rimraf build/ && cem analyze --litelement --globs \"src/components/**/*.ts\" && vite build && eleventy --config=.eleventy.cjs && cp -r ./public/* ./dist/docs",
    "build:components": "pnpm -C src/components run build",
    "publish:components": "pnpm -C src/components run publish:components",
    "test": "pnpm test:unit; pnpm test:e2e",
    "test:unit": "playwright test components/",
    "test:e2e": "playwright test tests/",
    "lint": "eslint ."
  },
  "customElements": "custom-elements.json",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@custom-elements-manifest/analyzer": "^0.9.4",
    "@playwright/test": "^1.41.0",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@sand4rt/experimental-ct-web": "^1.41.0",
    "@types/node": "^20.11.30",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "eslint": "^8.57.0",
    "lit": "^3.1.2",
    "rimraf": "^5.0.5",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^2.0.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^7.3.1",
    "vite": "^4.4.7",
    "vite-plugin-cem": "^0.7.0"
  }
}
